{
  "name": "pythia",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-core": "ng build @myrmidon/pythia-core && ng build @myrmidon/pythia-material",
    "build-api": "ng build @myrmidon/pythia-api",
    "build-ui": "ng build @myrmidon/pythia-ui && ng build @myrmidon/pythia-stats",
    "build-doc-reader": "ng build @myrmidon/pythia-document-reader && ng build @myrmidon/pythia-document-reader-page",
    "build-doc-list": "ng build @myrmidon/pythia-document-list && ng build @myrmidon/pythia-document-list-page",
    "build-term-list": "ng build @myrmidon/pythia-term-list && ng build @myrmidon/pythia-term-list-page",
    "build-corpus-list": "ng build @myrmidon/pythia-corpus-list && ng build @myrmidon/pythia-corpus-list-page",
    "build-search": "ng build @myrmidon/pythia-search && ng build @myrmidon/pythia-search-page",
    "build-lib": "npm run build-core && npm run build-api && npm run build-ui && npm run build-doc-reader && npm run build-doc-list && npm run build-term-list && npm run build-corpus-list && npm run build-search"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.4",
    "@angular/cdk": "^12.2.4",
    "@angular/common": "~12.2.4",
    "@angular/compiler": "~12.2.4",
    "@angular/core": "~12.2.4",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "~12.2.4",
    "@angular/material": "^12.2.4",
    "@angular/platform-browser": "~12.2.4",
    "@angular/platform-browser-dynamic": "~12.2.4",
    "@angular/router": "~12.2.4",
    "@auth0/angular-jwt": "^5.0.2",
    "@datorama/akita": "^6.2.3",
    "@datorama/akita-ngdevtools": "^6.0.1",
    "@myrmidon/auth-jwt-admin": "0.0.2",
    "@myrmidon/auth-jwt-login": "0.0.2",
    "@myrmidon/ng-tools": "0.0.2",
    "gravatar": "^1.8.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.4",
    "@angular/cli": "~12.2.4",
    "@angular/compiler-cli": "~12.2.4",
    "@types/gravatar": "^1.8.3",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^12.1.0",
    "typescript": "~4.3.2"
  }
}
